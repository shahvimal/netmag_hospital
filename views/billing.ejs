                                            <div class="col-md-12">
                                                <div class="form-group">
                                                    <label>Amount</label>
                                                    <textarea rows="1" class="form-control" placeholder="$3000" value="Mike"></textarea>
                                                </div>
                                            </div>
                                        </div>
    
